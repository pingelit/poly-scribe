    struct {{ def.name }} {{ ": public {}".format(def.inheritance) if def.inheritance }}
    {
        {% filter indent(width=8,first=True) %}
        {% include "members.jinja" %}

        {% include "serialize_func.jinja" %}
        {% endfilter +%}
    }
    {% if not loop.last %}

    {% endif %}
    {% endfor %}
}
